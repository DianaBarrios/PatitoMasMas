programa patito;
var
  int det;
  int matriz1[2][2], matriz2[2][2];
  int matriz3[3][3],matriz4[3][3],matriz5[3][3],matriz6[3][3],matriz7[3][3];
funcion int asignarmatriz5(int b);
var int i,j;
int mat[3][3];
{
  desde i=0 hasta 3 hacer {
    desde j=0 hasta 3 hacer {
      mat[i][j] = b * 3;
      b = b + 1;
    }
  }
  regresa(mat);
}
principal(){
  matriz3 = asignarmatriz5(5);
  escribe(matriz3);
}
